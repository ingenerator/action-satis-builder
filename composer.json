{
  "require": {
    "composer/satis": "3.x@dev",
    "fitchlearning/satisfy": "dev-acoulton-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/acoulton/satisfy"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "8.2",
      "ext-openssl": "0"
    },
    "vendor-dir": "builder/vendor",
    "allow-plugins": {
      "composer/satis": true
    }
  }
}
